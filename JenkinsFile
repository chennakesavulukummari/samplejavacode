pipeline {
    agent any
    stages {
        stage('STEP-1 : Clean') {
            steps {
                sh 'mvn clean'
            }
        }
    stage('STEP-2 : Validate') {
            steps {
                sh 'mvn validate'
            }
        }
    }
    post { 
        always { 
            echo 'I have Executed Clean & Validate Job'
        }
    }
}
